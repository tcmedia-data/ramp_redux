//COMBINE APNX Files into one MONTHLY
SELECT * FROM 
(TABLE_DATE_RANGE( ManagedServices_APNX.APPNEXUS_NETWORK_, TIMESTAMP('2016-06-01'), TIMESTAMP('2016-06-30')))
//(TABLE_DATE_RANGE( ManagedServices.APPNEXUS_NETWORK_JOIN_, TIMESTAMP('2016-06-01'), TIMESTAMP('2016-06-30')))

//Save Output to: ManagedServices_APNX.APPNEXUS_NETWORK_Monthly_20160x01
WHERE TransactionDate IS NOT NULL

