//FLOAT(Ad_server_CPM_and_CPC_revenue_CA) AS Ad_server_CPM_and_CPC_revenue_CA,
//FLOAT(Ad_server_CPD_revenue_CA) AS Ad_server_CPD_revenue_CA,
//FLOAT(AdSense_revenue_CA) AS AdSense_revenue_CA,
